{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.9" }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/", "dest": "/templates/index.html" },
    { "src": "/start", "dest": "/templates/start.html" },
    { "src": "/results", "dest": "/templates/results.html" },
    { "src": "/index_home", "dest": "/templates/index_home.html" },
    { "src": "/api/(.*)", "dest": "/api/index.py" }
  ]
}
